@model IEnumerable<pragim_dotNetWebApp.Models.Employee>
@{
  ViewBag.Title = "Employee List";
}
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Department</th>
    </tr>
  </thead>
  <tbody>
    @{
      foreach(pragim_dotNetWebApp.Models.Employee employee in Model) {
        <tr>
          <td>@employee.Id</td>
          <td>@employee.Name</td>
          <td>@employee.Department</td>
        </tr>
      }
    }
  </tbody>
</table>
{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MyKey": "Value of Mykey from appsettings.json",
  "ConnectionStrings": {
    "EmployeeApp": "server=(localdb)\\MSSQLLocalDB;database=EmployeeApp;Trusted_Connection=true"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "A:\\Documents\\VS\\pragim-dotNetWebApp\\pragim-dotNetWebApp\\DemoLogs\\nlog-${shortdate}.log"
      },
      "logconsole": {
        "type": "ColoredConsole"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile"
      }
    ]
  }
}